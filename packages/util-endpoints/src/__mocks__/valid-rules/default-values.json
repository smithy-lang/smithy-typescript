{
  "version": "1.0",
  "parameters": {
    "bar": {
      "type": "string",
      "documentation": "docs"
    },
    "baz": {
      "type": "string",
      "documentation": "docs",
      "required": true,
      "default": "baz"
    },
    "endpoint": {
      "type": "string",
      "builtIn": "SDK::Endpoint",
      "required": true,
      "default": "asdf",
      "documentation": "docs"
    }
  },
  "rules": [
    {
      "conditions": [
        {
          "fn": "isSet",
          "argv": [
            {
              "ref": "bar"
            }
          ]
        }
      ],
      "endpoint": {
        "url": "https://example.com/{baz}"
      },
      "type": "endpoint"
    },
    {
      "conditions": [],
      "documentation": "error fallthrough",
      "error": "endpoint error",
      "type": "error"
    }
  ]
}
